    "@types/react": "^18.0.38",
    "@types/react-dom": "^18.0.11",
    "@ya.praktikum/react-developer-burger-ui-components": "^1.14.0",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
